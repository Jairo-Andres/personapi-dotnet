@model personapi_dotnet.Models.Entities.Telefono

<h2>Crear Nuevo Teléfono</h2>

<form asp-action="Create">
    <div class="form-group">
        <label for="Num">Número</label>
        <input type="text" class="form-control" asp-for="Num" />
    </div>
    <div class="form-group">
        <label for="Oper">Operador</label>
        <input type="text" class="form-control" asp-for="Oper" />
    </div>
    <div class="form-group">
        <label for="Duenio">Dueño (CC)</label>
        <input type="number" class="form-control" asp-for="Duenio" />
    </div>
    <button type="submit" class="btn btn-primary">Guardar</button>
</form>
