@model personapi_dotnet.Models.Entities.Estudio

<h2>Editar Estudio</h2>

<form asp-action="Edit">
    <input type="hidden" asp-for="IdProf" />
    <input type="hidden" asp-for="CcPer" />
    <div class="form-group">
        <label for="Fecha">Fecha</label>
        <input type="date" class="form-control" asp-for="Fecha" />
    </div>
    <div class="form-group">
        <label for="Univer">Universidad</label>
        <input type="text" class="form-control" asp-for="Univer" />
    </div>
    <button type="submit" class="btn btn-primary">Guardar</button>
</form>
