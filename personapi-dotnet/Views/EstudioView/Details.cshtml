@model personapi_dotnet.Models.Entities.Estudio

<h2>Detalles del Estudio</h2>

<div class="form-group">
    <label>Id Profesión:</label>
    <p>@Model.IdProf</p>
</div>
<div class="form-group">
    <label>Cédula Persona:</label>
    <p>@Model.CcPer</p>
</div>
<div class="form-group">
    <label>Fecha:</label>
    <p>@Model.Fecha</p>
</div>
<div class="form-group">
    <label>Universidad:</label>
    <p>@Model.Univer</p>
</div>

<a asp-action="Edit" asp-route-idProf="@Model.IdProf" asp-route-ccPer="@Model.CcPer">Editar</a> |
<a asp-action="Index">Volver a la lista</a>
